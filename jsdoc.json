{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": [".", "README.md", "package.json"],
        "includePattern": "test.js$",
        "excludePattern": "docs"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "remoteFileExplorer",
        "copyright": "2017",
        "linenums": true,
        "collapseSymbols": true
    },
    "markdown"  : {
        "parser"        : "gfm",
        "hardwrap"      : false,
        "idInHeadings"  : true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/sutro-jsdoc-theme"
    }
}